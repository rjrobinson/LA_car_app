<h1> List of Manufacturers </h1>

<ul>
  <% @manufacturers.each do |manufacturer| %>
    <li><%=  link_to manufacturer.name, manufacturer_path(manufacturer) %> -- <%= manufacturer.country %></li>
  <% end %>
</ul>

<p><%= link_to 'Add new manufacturer', new_manufacturer_path, class: 'manufacturer' %></p>
